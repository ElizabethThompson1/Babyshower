{
  "rewrites": [
    {
      "source": "/",
      "destination": "/invitation.html"
    },
    {
      "source": "/index.html",
      "destination": "/invitation.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}

